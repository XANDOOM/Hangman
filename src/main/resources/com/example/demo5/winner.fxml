<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo5.WinnerController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="608.0" layoutX="-1.0" pickOnBounds="true">
         <image>
            <Image url="@youwin.png" />
         </image>
      </ImageView>
      <Button layoutX="325.0" layoutY="350.0" mnemonicParsing="false" onAction="#onRestartButton" style="-fx-background-color: yellow;" text="Play Again">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="190.0" layoutY="350.0" mnemonicParsing="false" onAction="#onExitButton" prefHeight="26.0" prefWidth="55.0" style="-fx-background-color: yellow;" text="Exit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
